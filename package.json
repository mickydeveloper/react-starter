  "main": "./dist/bundle.js",
  "scripts": {
    "build": "export API=http://localhost:3000 && webpack --config configs/webpack/webpack.local.js --display-error-details",
    "build:prod": "webpack --config configs/webpack/webpack.prod.js --display-error-details",
    "start": "export API=http://localhost:3000 && npm run build && npm run wds",
    "wds": "webpack-dev-server --config configs/webpack/webpack.local.js",
    "test": "export API=http://localhost:3000&& jest",
    "test:format:json": "export API=http://localhost:3000&&jest --json",
    "test:update": "jest -u",
    "test:watch": "export API=http://localhost:3000&& jest --watch",
    "e2e": "ts-node --project test/tsconfig.test.json test/e2e/test.ts"
  },
  "jest": {
    "setupFiles": [
      "./test/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "cobertura"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testURL": "http://localhost",
    "testPathIgnorePatterns": [
      "<rootDir>/test/e2e/test.ts"
    ]
  },
